import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		// Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		
		for (int i = 0; i < n; i++) {
			
			 double a = sc.nextInt();
			 double b = sc.nextInt();
			 double c = sc.nextInt();
			 
			 double media = (a * 2.0 + b * 3.0 + c * 5.0) / 10.00;
			 
			 System.out.printf("%.1f%n", media);
			}
		
		
		sc.close();
	}

}
