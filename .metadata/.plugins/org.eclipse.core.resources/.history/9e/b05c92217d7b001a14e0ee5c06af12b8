import java.util.Scanner;

public class Main{
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
						
		double valor;
		
		System.out.println("Digite um valor: ");
		valor = sc.nextDouble();
		
		
		if(valor >= 0 || valor <= 25) {
			System.out.println("O valor digitado está no intervalo de [0,25]");
		} 
			else if (valor >= 25 || valor <= 50){
				System.out.println("O valor digitado está no intervalo de [25,50]");
			}
			else if (valor >= 50 || valor <= 75){
				System.out.println("O valor digitado está no intervalo de [50,75]");
			}
			else if (valor >= 75 || valor <= 100){
				System.out.println("O valor digitado está no intervalo de [75,100]");
			}
		
		else{
			System.out.println("Fora de intervalo");
		}
		
		sc.close();
	}
}
